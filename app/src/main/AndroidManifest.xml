<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.ternavest">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.SYSTEM_ALERT_WINDOW" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_logo"
        android:supportsRtl="true"
        android:theme="@style/Theme.Ternavest"
        tools:ignore="AllowBackup">
        <activity android:name=".ui.both.profile.TentangAplikasi"/>
        <activity android:name=".ui.both.login.ResetPasswordActivity" />
        <activity android:name=".ui.both.login.RegisterActivity" />
        <activity android:name=".ui.both.login.LoginActivity" />
        <activity android:name=".ui.both.welcome.WelcomeActivity" />
        <activity android:name=".ui.both.main.MainActivity" />
        <activity android:name=".ui.investor.portfolio.PaymentActivity" />
        <activity android:name=".ui.investor.portfolio.AddUpdatePortfolioActivity" />
        <activity android:name=".ui.both.portfolio.DetailPortfolioActivity" />
        <activity android:name=".ui.peternak.kelola.laporan.EditLaporanActivity" />
        <activity android:name=".ui.peternak.kelola.laporan.TambahLaporanActivity" />
        <activity android:name=".ui.peternak.kelola.proyek.EditProyekActivity" />
        <activity android:name=".ui.peternak.kelola.laporan.LaporanActivity" />
        <activity android:name=".ui.both.profile.SettingsActivity" />
        <activity android:name=".ui.investor.home.LaporanHomeActivity" />
        <activity android:name=".ui.both.profile.DetailProfileActivity" />
        <activity android:name=".testing.WilayahTest" />
        <activity android:name=".ui.peternak.kelola.proyek.TambahProyekActivity" />
        <activity android:name=".ui.both.welcome.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service android:name=".testing.notification.service.MyFirebaseMessaging">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT"/>
            </intent-filter>
        </service>
    </application>
</manifest>